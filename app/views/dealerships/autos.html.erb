<h1>Dealership's Vehicles</h1>

<% @vehicles.each do |vehicle| %>
  <h4><a href="/vehicles/<%= vehicle.id %>"><%= vehicle.name =%></a></h4>
  <ul>
    <li>Passenger Limit: <%= vehicle.passenger_limit %></li>
    <li>Automatic Tranmission: <%= vehicle.auto_transmission %></li>
    <li>Vehicle Id: <%= vehicle.id %></li>
    <li>Created At: <%= vehicle.created_at %></li>
    <li>Updated At:<%= vehicle.updated_at %></li>
  </ul>
<% end %>
 <p><a href="/dealerships/<%= @dealership_id %>/autos/new_auto">New Vehicle</a></p>
